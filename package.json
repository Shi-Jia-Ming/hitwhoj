{
  "private": true,
  "name": "hitwhoj",
  "scripts": {
    "build": "sass --no-source-map styles:app/styles && remix build",
    "clean": "rimraf public/build server/build app/styles .cache",
    "dev": "concurrently -n \"REMIX,SASS,NODE\" \"remix watch\" \"sass --no-source-map --watch styles:app/styles\" \"yarn start:dev\"",
    "fmt": "prettier --write . && prisma format",
    "postinstall": "remix setup node",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "start:dev": "cross-env NODE_ENV=development node server/index.js"
  },
  "dependencies": {
    "@prisma/client": "^3.9.1",
    "@remix-run/express": "^1.1.3",
    "@remix-run/react": "^1.1.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "prettier": "^2.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "remix": "^1.1.3"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.1.3",
    "@types/react": "^17.0.24",
    "@types/react-dom": "^17.0.9",
    "concurrently": "^7.0.0",
    "prisma": "^3.9.1",
    "rimraf": "^3.0.2",
    "sass": "^1.49.7",
    "typescript": "^4.1.2"
  },
  "engines": {
    "node": "=16"
  },
  "sideEffects": false
}
